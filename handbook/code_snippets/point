// a, b line; c point
ll cross(pii a, pii b, pii c){
    pii va, vb;
    va.first = b.first-a.first;
    va.second = b.second-a.second;
    vb.first = c.first-b.first;
    vb.second = c.second-b.second;
    return (ll)va.first*vb.second - (ll)vb.first*va.second;
}

ll cros = cross(a, b, p);
if(cros > 0)
    cout << "LEFT\n";
else if(cros == 0)
    cout << "TOUCH\n";
else
    cout << "RIGHT\n";
}


